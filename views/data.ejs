<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Patients and Providers Data</title>
</head>
<body>
    <div class="container mt-5">
        <h1>Patients</h1>
        <table class="table">
            <thead>
                <tr>
                    <th>Patient ID</th>
                    <th>First Name</th>
                    <th>Last Name</th>
                    <th>Date of Birth</th>
                </tr>
            </thead>
            <tbody>
                <% patients.forEach(function(patient) { %>
                    <tr>
                        <td><%= patient.patient_id %></td>
                        <td><%= patient.first_name %></td>
                        <td><%= patient.last_name %></td>
                        <td><%= patient.date_of_birth.toISOString().split('T')[0] %></td>
                    </tr>
                <% }); %>
            </tbody>
        </table>

        <h1>Providers</h1>
        <table class="table">
            <thead>
                <tr>
                    <th>First Name</th>
                    <th>Last Name</th>
                    <th>Provider Specialty</th>
                </tr>
            </thead>
            <tbody>
                <% providers.forEach(function(provider) { %>
                    <tr>
                        <td><%= provider.first_name %></td>
                        <td><%= provider.last_name %></td>
                        <td><%= provider.provider_specialty %></td>
                    </tr>
                <% }); %>
            </tbody>
        </table>
    </div>
</body>
</html>
